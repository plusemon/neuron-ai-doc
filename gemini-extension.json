{
  "name": "neuron-ai-mcp-extension",
  "version": "1.0.0",
  "description": "An MCP extension for Gemini CLI built with Neuron AI framework.",
  "author": "Plusemon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/plusemon/neuron-ai-doc.git"
  },
  "mcpServers": {
    "neuron-ai-local": {
      "type": "stdio",
      "command": "php",
      "args": [
        "${extensionPath}/server.php"
      ]
    },
    "neuron-ai-doc": {
      "type": "http",
      "url": "https://docs.neuron-ai.dev/~gitbook/mcp"
    }
  }
}
